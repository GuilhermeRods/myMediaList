  "bugs": {
    "url": "https://github.com/GuilhermeRods/myMediaList/issues"
  },
  "homepage": "https://github.com/GuilhermeRods/myMediaList#readme",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^8.2.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "knex": "^0.21.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "path": "^0.12.7",
    "pg": "^8.0.3",
    "sqlite3": "^4.2.0",
    "standard": "^14.3.3"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.3",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
