{
  "extends": ["prettier/standard"],
  "parser": "babel-eslint",
  "plugins": ["import", "prettier", "standard"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "no-trailing-spaces": [2],
    "prettier/prettier": 2,
    "space-before-function-paren": 0,
    "linebreak-style": "warn"
  }
}
